{
	"name": "SignETS plugin",
	"manifest_version": 2,
	"version": "0.0.7",
	"description": "Une extension pour am√©liorer l'interface de SignETS",
	"icons": {
		"128": "assets/icon128.png",
		"48": "assets/icon48.png",
		"16": "assets/icon16.png"
	},
	"browser_action": {
		"default_icon": "assets/icon16.png",
		"default_popup": "popup.html"
	},
	"content_scripts": [{
		"matches": ["*://signets-ens.etsmtl.ca/*"],
		"js": [
			"content.js"
		]
	}],
	"permissions": [
		"storage"
	]
}