{
	"name": "SignETS plugin",
	"manifest_version": 2,
	"version": "0.2.5",
	"description": "Une extension pour am√©liorer l'interface de SignETS",
	"icons": {
		"128": "assets/icon128.png",
		"48": "assets/icon48.png",
		"16": "assets/icon16.png"
	},
	"browser_action": {
		"default_icon": "assets/icon16.png",
		"default_popup": "popup.html"
	},
	"content_scripts": [{
		"matches": ["*://signets-ens.etsmtl.ca/*"],
		"css": [
			"tippyjs/scale.css",
			"tippyjs/light.css",
			"tippyjs/light-border.css",
			"chartjs/Chart.min.css"
		],
		"js": [
			"tippyjs/popper.min.js",
			"tippyjs/tippy-bundle.umd.min.js",
			"chartjs/Chart.min.js",
			"content.js"
		]
	},{
		"matches": ["*://signets-ens.etsmtl.ca/*"],
		"js": [
			"changeTheme.js"
		],
		"run_at": "document_start"
	}],
	"web_accessible_resources": [
		"assets/pavillonE.jpg"
	  ],
	"permissions": [
		"storage"
	]
}